{
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "typescript-plugin-toml"
      }
    ],
    "types": ["vite/client"],
    "moduleResolution": "node",
    "allowJs": true,
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}